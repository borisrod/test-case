{
    "name": "antimattr/test-case",
    "type": "library",
    "description": "PHPUnit Test Case Assistance",
    "keywords": ["antimattr", "phpunit"],
    "homepage": "http://github.com/antimattr/test-case",
    "license": "MIT",
    "authors": [
        {"name": "Matthew Fitzgerald", "email": "matthewfitz@gmail.com"}
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fabpot/php-cs-fixer": "0.5.*@dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "AntiMattr\\TestCase\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },      
    "archive": {
        "exclude": ["bin"]
    }    
}
